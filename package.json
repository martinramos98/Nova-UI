{
	"name": "nova-js-ui",
	"version": "0.1.0",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.2.14"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-vercel": "^5.3.2",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/postcss": "4.0.0-alpha.16",
		"@types/eslint": "^8.56.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0-next.4",
		"highlight.js": "^11.9.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"publint": "^0.1.9",
		"svelte": "5.0.0-next.183",
		"svelte-check": "^3.6.0",
		"svelte-highlight": "^7.6.1",
		"tailwindcss": "4.0.0-alpha.16",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.1.7"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@nova/accordion": "workspace:*",
		"@nova/alert":"workspace:*",
		"@nova/avatar":"workspace:*",
		"@nova/badge":"workspace:*",
		"@nova/button": "workspace:*",
		"@nova/callout":"workspace:*",
		"@nova/card":"workspace:*",
		"@nova/chip":"workspace:*",
		"@nova/collapsable-block":"workspace:*",
		"@nova/context-menu":"workspace:*",
		"@nova/divider":"workspace:*",
		"@nova/drawer":"workspace:*",
		"@nova/dropdown":"workspace:*",
		"@nova/floating-subsection":"workspace:*",
		"@nova/icon":"workspace:*",
		"@nova/image":"workspace:*",
		"@nova/input":"workspace:*",
		"@nova/link":"workspace:*",
		"@nova/loader":"workspace:*",
		"@nova/modal":"workspace:*",
		"@nova/navbar":"workspace:*",
		"@nova/popover":"workspace:*",
		"@nova/progress":"workspace:*",
		"@nova/radio":"workspace:*",
		"@nova/selection":"workspace:*",
		"@nova/slider":"workspace:*",
		"@nova/switch":"workspace:*",
		"@nova/table":"workspace:*",
		"@nova/tabs":"workspace:*",
		"@nova/title":"workspace:*",
		"@nova/toast":"workspace:*",
		"@nova/tooltip":"workspace:*",
		"@nova/element-animation-js": "workspace:*"
	},
	"workspaces": [
		"packages/*"
	],
	"keywords": [
		"svelte",
		"sveltejs",
		"ui",
		"component",
		"library",
		"component-library",
		"svelte ui",
		"nova ui",
		"nova"
	]
}
