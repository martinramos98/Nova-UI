{
	"name": "nova-js-ui",
	"version": "0.1.3",
	"license": "MIT",
	"scripts": {
		"dev": "dotenvx run -f .env.dev -- vite dev",
		"build": "dotenvx run -f .env.prod -- vite build && pnpm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "pnpm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.2.14"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-vercel": "^5.4.5",
		"@sveltejs/kit": "^2.6.1",
		"@sveltejs/package": "^2.3.2",
		"@sveltejs/vite-plugin-svelte": "4.0.0-next.7",
		"@tailwindcss/postcss": "4.0.0-alpha.25",
		"@types/eslint": "^8.56.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"dotenv": "^16.4.5",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "2.36.0-next.13",
		"highlight.js": "^11.9.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"publint": "^0.1.9",
		"svelte": "5.0.0-next.260",
		"svelte-adapter-deno": "^0.9.1",
		"svelte-check": "^3.6.0",
		"svelte-highlight": "^7.6.1",
		"tailwindcss": "4.0.0-alpha.25",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.4.9"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@nv-org/element-animation-js": "workspace:*"
	},
	"workspaces": [
		"packages/*",
		"packages/components/*"
	],
	"keywords": [
		"svelte",
		"sveltejs",
		"ui",
		"component",
		"library",
		"component-library",
		"svelte ui",
		"nova ui"
	],
	"packageManager": "pnpm@8.7.5"
}
