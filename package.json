{
	"name": "nova-js-ui",
	"version": "0.1.3",
	"license": "MIT",
	"scripts": {
		"dev": "dotenvx run -f .env.dev -- vite dev",
		"build": "dotenvx run -f .env.prod -- vite build && pnpm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "pnpm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^5.2.8"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/adapter-vercel": "^5.4.8",
		"@sveltejs/kit": "^2.8.5",
		"@sveltejs/package": "^2.3.7",
		"@sveltejs/vite-plugin-svelte": "5.0.1",
		"@tailwindcss/postcss": "4.0.0-beta.7",
		"@types/eslint": "^9.6.1",
		"@typescript-eslint/eslint-plugin": "^8.16.0",
		"@typescript-eslint/parser": "^8.16.0",
		"dotenv": "^16.4.5",
		"eslint": "^9.15.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "2.46.0",
		"highlight.js": "^11.10.0",
		"prettier": "^3.4.1",
		"prettier-plugin-svelte": "^3.3.2",
		"publint": "^0.2.12",
		"svelte": "5.2.9",
		"svelte-adapter-deno": "^0.9.1",
		"svelte-check": "^4.1.0",
		"svelte-highlight": "^7.7.0",
		"tailwindcss": "4.0.0-beta.7",
		"tslib": "^2.8.1",
		"typescript": "^5.7.2",
		"vite": "^6.0.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@nv-org/element-animation-js": "workspace:*"
	},
	"workspaces": [
		"packages/*",
		"packages/components/*"
	],
	"keywords": [
		"svelte",
		"sveltejs",
		"ui",
		"component",
		"library",
		"component-library",
		"svelte ui",
		"nova ui"
	],
	"packageManager": "pnpm@9.14.2"
}
