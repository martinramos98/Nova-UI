[
	{
		"name": "class?:string",
		"description": "HTML Class attribute for drawer.",
		"required": false,
		"default": "''"
	},
	{
		"name": "onClickTrigger?: () => void",
		"description": "Function to handle click events on the trigger.",
		"required": false,
		"default": "undefined"
	},
	{
		"name": "children?: Snippet<[any]>",
		"description": "Children elements of the dropdown.",
		"required": false,
		"default": "undefined"
	},
	{
		"name": "offset?: number",
		"description": "Offset value for the dropdown position.",
		"required": false,
		"default": 5
	},
	{
		"name": "triggerText?: string",
		"description": "Text for the dropdown trigger.",
		"required": false,
		"default": "''"
	},
	{
		"name": "position?: string",
